{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/react-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "abdulmajeed-jamaan/filament-translatable-tabs": "^5.0",
        "filament/filament": "^5.0",
        "filament/spatie-laravel-media-library-plugin": "^5.2",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/ai": "^0.2.1",
        "laravel/fortify": "^1.30",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.9",
        "league/flysystem-aws-s3-v3": "^3.31",
        "pxlrbt/filament-activity-log": "^2.1",
        "spatie/eloquent-sortable": "^5.0",
        "spatie/laravel-activitylog": "^4.11",
        "spatie/laravel-medialibrary": "^11.20",
        "spatie/laravel-permission": "^6.24",
        "spatie/laravel-translatable": "^6.12",
        "stechstudio/filament-impersonate": "^5.0"
    },
    "require-dev": {
        "driftingly/rector-laravel": "^2.1",
        "elegantly/laravel-translator": "^3.4",
        "fakerphp/faker": "^1.23",
        "fruitcake/laravel-debugbar": "^4.0",
        "larastan/larastan": "^3.0",
        "laravel/boost": "^2.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.52",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-laravel": "^4.0",
        "spatie/laravel-typescript-transformer": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "pre": [
            "composer format",
            "composer refactor",
            "composer analyse",
            "php artisan translator:untranslated en he --translate",
            "php artisan app:check-missing-translations --all",
            "php artisan typescript:transform",
            "npm run types",
            "npm run format",
            "composer test:coverage"
        ],
        "translate": [
            "php artisan app:sync-translations",
            "php artisan translator:untranslated en he --translate"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1 --timeout=0\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "format:check": [
            "./vendor/bin/pint --dirty --test"
        ],
        "format": [
            "./vendor/bin/pint"
        ],
        "analyse": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "refactor:dry-run": [
            "rector process --dry-run"
        ],
        "refactor": [
            "rector process",
            "./vendor/bin/pint"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "test:coverage": [
            "php -d pcov.enabled=1 ./vendor/bin/pest --coverage --min=80"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan boost:update --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
